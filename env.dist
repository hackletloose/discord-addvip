# env.dist
# ---------
# Copy or rename this file to ".env" and fill in the values.
# Never commit real tokens or passwords to the repository!

# Your bot token from the Discord Developer Portal
DISCORD_BOT_TOKEN=

# Base URL for your API backend, e.g., https://api.example.com
API_BASE_URL=

# API token
API_TOKEN=

# The channel ID in Discord where the bot posts the "Grant VIP" button
ALLOWED_CHANNEL_ID=

# The channel ID in Discord where the bot logs each VIP grant
VIP_LOG_CHANNEL_ID=

# Language code for language.json, e.g., "en" or "de"
LANG_CODE=en
